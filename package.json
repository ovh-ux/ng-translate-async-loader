{
  "name": "@ovh-ux/ng-translate-async-loader",
  "version": "2.0.3",
  "description": "Angular translate asynchronous loader",
  "keywords": [
    "angularjs",
    "asynchronous",
    "loader",
    "ovh",
    "ovhcloud",
    "translate"
  ],
  "repository": "ovh-ux/ng-translate-async-loader",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/umd/ng-translate-async-loader.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production",
    "dev": "rollup -cw --environment BUILD:development",
    "lint": "run-p lint:js lint:md",
    "lint:js": "eslint --quiet --fix --format=pretty src",
    "lint:md": "eslint --quiet --fix --format=pretty --ext .md .",
    "prepare": "yarn run build",
    "test": "yarn run lint",
    "version": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-angular": "^8.2.0",
    "@ovh-ux/component-rollup-config": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-markdown": "^1.0.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5"
  },
  "peerDependencies": {
    "angular": "^1.5.0"
  }
}
